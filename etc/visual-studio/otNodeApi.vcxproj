  <ItemGroup>
    <ClCompile Include="..\..\examples\drivers\windows\otNodeApi\dllmain.cpp" />
    <ClCompile Include="..\..\examples\drivers\windows\otNodeApi\otNodeApi.cpp" />
  </ItemGroup>
  <ItemGroup>
